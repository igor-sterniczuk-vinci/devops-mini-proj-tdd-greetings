{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": ["error", "always"],
    "no-console": "warn",
    "quotes": "off",
    "camelcase": "off",
    "no-underscore-dangle": "off",
    "no-unused-vars": "off",
    "no-eval": "off",
    "no-template-curly-in-string": "off",
    "no-multi-assign": "off",
    "indent": "off"
  }
}
