{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": ["error", "always"],
    "no-console": "warn",
    "quotes": "off",
    "camelcase": "off",
    "no-underscore-dangle": "off",
    "no-unused-vars": "off",
    "no-eval": "off",
    "no-template-curly-in-string": "off",
    "no-multi-assign": "off",
    "indent": "off",
    "no-shadow": "off",
    "func-names": "off",
    "no-param-reassign": "off",
    "no-nested-ternary": "off",
    "default-case": "off",
    "no-return-assign": "off",
    "no-cond-assign": "off",
    "no-void": "off",
    "no-sequences": "off"
  }
}
